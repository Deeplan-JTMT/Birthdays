{
  "name": "birthdays",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.13.0 <17.0.0 || >=18.17.1 <19.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "node ./services/updateVersion.service.js npm run clean && npm run bundle && npm run package-solution && npm run open-explorer",
    "clean": "gulp clean",
    "bundle": "gulp bundle --ship --continueOnError",
    "package-solution": "gulp package-solution --ship",
    "open-explorer": "start sharepoint\\solution",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./services/deployApp.service.ps1",
    "start": "gulp bundle --custom-serve --max_old_space_size=4096 && fast-serve",
    "serve": "fast-serve"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fluentui/react": "^8.106.4",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@microsoft/sp-component-base": "1.18.2",
    "@microsoft/sp-core-library": "1.18.2",
    "@microsoft/sp-lodash-subset": "1.18.2",
    "@microsoft/sp-office-ui-fabric-core": "1.18.2",
    "@microsoft/sp-property-pane": "1.18.2",
    "@microsoft/sp-webpart-base": "1.18.2",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@pnp/sp": "^3.21.0",
    "@pnp/spfx-property-controls": "^3.15.1",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.4",
    "moment": "^2.30.1",
    "office-ui-fabric-react": "^7.204.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-slick": "^0.30.2",
    "react-slideshow-image": "^4.0.4",
    "reactstrap": "^9.2.3",
    "sweetalert2": "^11.22.0",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.18.2",
    "@microsoft/eslint-plugin-spfx": "1.18.2",
    "@microsoft/rush-stack-compiler-4.7": "0.1.0",
    "@microsoft/sp-build-web": "1.18.2",
    "@microsoft/sp-module-interfaces": "1.18.2",
    "@rushstack/eslint-config": "2.5.1",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/webpack-env": "~1.15.2",
    "ajv": "^6.12.5",
    "eslint": "8.7.0",
